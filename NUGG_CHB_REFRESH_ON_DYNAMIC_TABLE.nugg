      INTO TABLE et_data
     WHERE parameter_id = zcl_wm_parm_inv_strg_lctn_prit=&gt;c_inv_stg_lct_prt
     ORDER BY priority.
    IF sy-subrc &lt;&gt; 0.
      RAISE EXCEPTION TYPE cx_sy_sql_error.
    ENDIF.
  ENDMETHOD.                    &quot;query_ztbwm_pm_inv_slp
ENDCLASS.                    &quot;lcl_db IMPLEMENTATION</localImplementation>
